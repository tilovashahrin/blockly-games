        <target>הסתרת הצב</target>
      </trans-unit>
      <trans-unit id="5355893896926183437" datatype="html">
        <target>בשלב זה, נדרש ממך לערום את כל הקוביות שבמשטח העבודה הלבן.</target>
      </trans-unit>
      <trans-unit id="4138774728570944645" datatype="html">
        <target>נוע קדימה</target>
