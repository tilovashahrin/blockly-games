      <trans-unit id="3293197141663117591" datatype="html">
        <target>यदि पथ आगे है तो</target>
      </trans-unit>
      <trans-unit id="1972926420229186759" datatype="html">
        <target>सूची</target>
      </trans-unit>
      <trans-unit id="295126337553066153" datatype="html">
        <target>पेन नीचे करें</target>
      </trans-unit>
      <trans-unit id="7512355766258181728" datatype="html">
        <target>मूंछ</target>
      </trans-unit>
      <trans-unit id="1659902989275631885" datatype="html">
        <target>फर</target>
      </trans-unit>
      <trans-unit id="8501578350336555138" datatype="html">
        <target>तालाब</target>
      </trans-unit>
      <trans-unit id="3406853780244170069" datatype="html">
        <target>खोल</target>
      </trans-unit>
      <trans-unit id="5561607146816569193" datatype="html">
        <target>पीला सितारा बनाएँ</target>
      </trans-unit>
      <trans-unit id="6668207685173746387" datatype="html">
        <target>एक 'अगर' ब्लॉक केवल तभी कुछ करेगा जब स्थिति सही हो। यदि बाईं तरफ एक पथ है तो बाएं मुड़ने का प्रयास करें।</target>
      </trans-unit>
      <trans-unit id="5922351303839855212" datatype="html">
        <target>करें</target>
      </trans-unit>
      <trans-unit id="4016533017150562878" datatype="html">
        <target>आपने इस स्तर के सभी ब्लॉकों का इस्तेमाल किया है। एक नया ब्लॉक बनाने के लिए, आपको पहले एक मौजूदा ब्लॉक को हटाना होगा।</target>
      </trans-unit>
      <trans-unit id="4502373380845009974" datatype="html">
        <target>खिलाड़ी को 90 डिग्री बाईं या दाईं तरफ घुमाता है।.</target>
      </trans-unit>
      <trans-unit id="2409415650297366191" datatype="html">
        <target>फ़ॉन्ट का माप</target>
      </trans-unit>
      <trans-unit id="1124966775678993568" datatype="html">
        <target>रीसेट करें</target>
      </trans-unit>
      <trans-unit id="2075728723517768097" datatype="html">
        <target>ब्लॉक जो बतातें हैं उसे खिलाड़ी से करवाता है।.</target>
      </trans-unit>
      <trans-unit id="4294783477990312138" datatype="html">
        <target>इतने तक पीछे हटें</target>
      </trans-unit>
      <trans-unit id="2801052495540198573" datatype="html">
        <target>दाएँ मुड़ें</target>
      </trans-unit>
      <trans-unit id="8787968467443938994" datatype="html">
        <target>फिर से शुरू करना चाहते हैं?</target>
      </trans-unit>
      <trans-unit id="1364178431368067261" datatype="html">
        <target>लक्ष्य</target>
      </trans-unit>
      <trans-unit id="4340252833994146197" datatype="html">
        <target>टेक्स्ट</target>
      </trans-unit>
      <trans-unit id="7726741685578327869" datatype="html">
        <target>आपके सारे हल मिटा दें?</target>
      </trans-unit>
      <trans-unit id="6014527610997525213" datatype="html">
        <target>प्रत्येक जानवर (हरा) के लिए, इसकी तस्वीर संलग्न करें, इसकी पैरों की संख्या चुनें, और अपने गुणों का एक ढेर बनाएं।</target>
      </trans-unit>
      <trans-unit id="1474725060640077068" datatype="html">
        <target>लक्षण:</target>
      </trans-unit>
      <trans-unit id="1189440630481795561" datatype="html">
        <target>अपना चित्र रेडिट में भेजें</target>
      </trans-unit>
      <trans-unit id="338593141197401350" datatype="html">
        <target>सहायता</target>
      </trans-unit>
      <trans-unit id="1038502666716911829" datatype="html">
        <target>तर्क</target>
      </trans-unit>
      <trans-unit id="200165623200379383" datatype="html">
        <target>आनुवंशिकी</target>
      </trans-unit>
      <trans-unit id="843223484015115436" datatype="html">
        <target>पक्षी</target>
      </trans-unit>
      <trans-unit id="6595415916414788836" datatype="html">
        <target>अधिक जानकारी...</target>
      </trans-unit>
      <trans-unit id="5367389302003162125" datatype="html">
        <target>चर</target>
      </trans-unit>
      <trans-unit id="5464369235708625567" datatype="html">
        <target>प्रोग्राम चलाएँ</target>
      </trans-unit>
      <trans-unit id="2353453760676853811" datatype="html">
        <target>बहुत बढ़िया!
सभी %1 ब्लॉक सही हैं।</target>
      </trans-unit>
      <trans-unit id="4168357856145700694" datatype="html">
        <target>तीन पीले सितारे और एक सफ़ेद रेखा बनाएँ</target>
      </trans-unit>
      <trans-unit id="7264063713014053084" datatype="html">
        <target>%1 ब्लॉक गलत हैं।</target>
      </trans-unit>
      <trans-unit id="1670227216063075975" datatype="html">
        <target>भूलभुलैया</target>
      </trans-unit>
      <trans-unit id="8983835283796130303" datatype="html">
        <target>रंग को इतना निर्धारित करें</target>
      </trans-unit>
      <trans-unit id="9137982570247930589" datatype="html">
        <target>अन्य</target>
      </trans-unit>
      <trans-unit id="4634715858389563104" datatype="html">
        <target>खिलाड़ी को एक स्थान आगे ले जाता है।.</target>
      </trans-unit>
      <trans-unit id="5718696793880573329" datatype="html">
        <target>https://en.wikipedia.org/wiki/Font</target>
      </trans-unit>
      <trans-unit id="5755229178435676524" datatype="html">
        <target>खेद है, '%1' किसी सेव किए गए प्रोग्राम से संबंधित नहीं है।</target>
      </trans-unit>
      <trans-unit id="7922752443358054817" datatype="html">
        <target>कछुए को (सर्कल और तीर) दृश्य या अदृश्य बनाता है।.</target>
      </trans-unit>
      <trans-unit id="4881180422989969508" datatype="html">
        <target>गैलरी देखें</target>
      </trans-unit>
      <trans-unit id="7834355790379776267" datatype="html">
        <target>अनुरोध के साथ समस्या हुई।</target>
      </trans-unit>
      <trans-unit id="6175401358049603739" datatype="html">
        <target>अगर निर्दिष्ट दिशा में एक पथ है, \nतो कार्य का पहला ब्लॉक करें। अन्यथा, \nकार्रवाई के दूसरे ब्लॉक करें। </target>
      </trans-unit>
      <trans-unit id="2827102143774531943" datatype="html">
        <target>उत्तर जांच करें</target>
      </trans-unit>
      <trans-unit id="8755460901437343901" datatype="html">
        <target>प्रिंट करें</target>
      </trans-unit>
      <trans-unit id="9149339265247850099" datatype="html">
        <target>संगीत</target>
      </trans-unit>
      <trans-unit id="4874525273235336318" datatype="html">
        <target>यदि निर्दिष्ट दिशा में एक पथ है, \nतो कुछ क्रियाएं करें। </target>
      </trans-unit>
      <trans-unit id="1190064103412776740" datatype="html">
        <target>समापन बिंदु को दोहराएं जब तक पूरा बिंदु तक पहुंच \nनहीं हो। </target>
      </trans-unit>
      <trans-unit id="153560734647047937" datatype="html">
        <target>कार्यक्रम को बंद करें और स्तर रीसेट करें।.</target>
      </trans-unit>
      <trans-unit id="8900931063490704792" datatype="html">
        <target>स्थिति जब पक्षी को कीड़ा नहीं मिला है।.</target>
      </trans-unit>
      <trans-unit id="3478101938294488773" datatype="html">
        <target>आप 'दोहराने' ब्लॉक के अंदर एक से अधिक ब्लॉक फिट कर सकते हैं।</target>
      </trans-unit>
      <trans-unit id="7327913532655152334" datatype="html">
        <target>रंग</target>
      </trans-unit>
      <trans-unit id="8678021904034212986" datatype="html">
        <target>यदि अन्य ब्लाक एक काम या अन्य काम करेंगे।</target>
      </trans-unit>
      <trans-unit id="6456907015338240672" datatype="html">
        <target>इस लिंक के साथ का अपने ब्लॉक का साझा करें:

 %1</target>
      </trans-unit>
      <trans-unit id="4272389616881657314" datatype="html">
        <target>सेव करें और ब्लॉक से लिंक करें।.</target>
      </trans-unit>
      <trans-unit id="8370361588013530358" datatype="html">
        <target>केवल दो ब्लॉकों का उपयोग करके इस रास्ते के अंत तक पहुंचें। एक बार से अधिक एक बार चलाने के लिए 'दोहराने' का प्रयोग करें</target>
      </trans-unit>
      <trans-unit id="1146327621590900127" datatype="html">
        <target>कछुआ छुपाएँ</target>
      </trans-unit>
      <trans-unit id="5355893896926183437" datatype="html">
        <target>इस स्तर पर, आपको सफेद कार्यक्षेत्र में सभी ब्लॉकों को एक साथ ढेर करने की आवश्यकता है।</target>
      </trans-unit>
      <trans-unit id="4138774728570944645" datatype="html">
        <target>आगे बढ़ें</target>
      </trans-unit>
      <trans-unit id="5509057833434715874" datatype="html">
        <target>यह स्तर बेहद मुश्किल है। क्या आप इसे छोड़ना चाहते हैं और अगले खेल पर जाना चाहेंगे? आप हमेशा बाद में वापस आ सकते हैं।</target>
      </trans-unit>
      <trans-unit id="4211607341272449380" datatype="html">
        <target>केवल चार खानों से एक सितारा बनाएँ।</target>
      </trans-unit>
      <trans-unit id="6889946611851664425" datatype="html">
        <target>इतने तक दाएँ मुड़ें</target>
      </trans-unit>
      <trans-unit id="7070128001410938826" datatype="html">
        <target>चोंच</target>
      </trans-unit>
      <trans-unit id="3794977698717555238" datatype="html">
        <target>पंख</target>
      </trans-unit>
      <trans-unit id="5683520235628284499" datatype="html">
        <target>इस मेन्यू से अपना पसंदीदा खिलाड़ी चुनें।</target>
      </trans-unit>
      <trans-unit id="515568367940150060" datatype="html">
        <target>प्रोसीजर</target>
      </trans-unit>
      <trans-unit id="3190825933229819894" datatype="html">
        <target>खिलाड़ी को भूलभुलैया के शुरू में वापस रखें।.</target>
      </trans-unit>
      <trans-unit id="2302415315348704045" datatype="html">
        <target>रेखा</target>
      </trans-unit>
      <trans-unit id="4245983571766649655" datatype="html">
        <target>चौड़ाई</target>
      </trans-unit>
      <trans-unit id="4130136547051161134" datatype="html">
        <target>फ़ॉन्ट</target>
      </trans-unit>
      <trans-unit id="6078028329780496695" datatype="html">
        <target>इतने तक बाएँ मुड़ें</target>
      </trans-unit>
      <trans-unit id="7404435315115664673" datatype="html">
        <target>आपने जो लिखा है, उसे रन करें।</target>
      </trans-unit>
      <trans-unit id="1240208832476690288" datatype="html">
        <target>डाटा साफ करें</target>
      </trans-unit>
      <trans-unit id="468604284020186650" datatype="html">
        <target>घेरा</target>
      </trans-unit>
      <trans-unit id="742817462675794226" datatype="html">
        <target>अवरोध</target>
      </trans-unit>
      <trans-unit id="7416117358889101130" datatype="html">
        <target>ओके</target>
      </trans-unit>
      <trans-unit id="1289975531431370979" datatype="html">
        <target>कछुआ</target>
      </trans-unit>
      <trans-unit id="3543744213961724053" datatype="html">
        <target>यदि आपके पास कीट है, या एक अलग शीर्षक अगर आपके पास कीट नहीं है, तो एक ब्लॉक में जाने के लिए इस ब्लॉक का उपयोग करें।</target>
      </trans-unit>
      <trans-unit id="5307840277183467457" datatype="html">
        <target>आपने 1 पंक्ति जावास्क्रिप्ट के साथ इस स्तर को हल किया:</target>
      </trans-unit>
      <trans-unit id="1737656799955544465" datatype="html">
        <target>आपने %1 स्तर जावास्क्रिप्ट पंक्ति के साथ इस स्तर को हल किया:</target>
      </trans-unit>
      <trans-unit id="945126989426193972" datatype="html">
        <target>ड्रॉइंग को बंद या चालू करने के लिए पेन को उपर या \nनीचे करता है। </target>
      </trans-unit>
      <trans-unit id="1554687224871598336" datatype="html">
        <target>क्या आप अगली चुनौती के लिए तैयार हैं?</target>
      </trans-unit>
      <trans-unit id="4197068989498547704" datatype="html">
        <target>क्या आप स्तर %1 के लिए तैयार हैं?</target>
      </trans-unit>
      <trans-unit id="1875380317327296159" datatype="html">
        <target>रद्द करें</target>
      </trans-unit>
      <trans-unit id="5287877014526685907" datatype="html">
        <target>आपकी सेव की गई फ़ाइल लोड नहीं हो सकी।  शायद यह ब्लॉकली के किसी भिन्न संस्करण के साथ बनाई गयी थी?</target>
      </trans-unit>
      <trans-unit id="699246884300242965" datatype="html">
        <target>शीर्षक</target>
      </trans-unit>
      <trans-unit id="5307089055470217701" datatype="html">
        <target>आपके पास %1 ब्लॉक बचें हैं।</target>
      </trans-unit>
      <trans-unit id="1794249676769577229" datatype="html">
        <target>आपके पास %0 ब्लॉक बचें हैं।</target>
      </trans-unit>
      <trans-unit id="561418461570337551" datatype="html">
        <target>आपके पास %2 ब्लॉक बचें हैं।</target>
      </trans-unit>
      <trans-unit id="337140251378156137" datatype="html">
        <target>भविष्य के प्रोग्रामरों के लिए बना खेल</target>
      </trans-unit>
      <trans-unit id="1141690494791308245" datatype="html">
        <target>ऊँचाई</target>
      </trans-unit>
      <trans-unit id="8496210457380227143" datatype="html">
        <target>यदि पथ दाएँ है तो</target>
      </trans-unit>
      <trans-unit id="2149839397262503913" datatype="html">
        <target>यदि पथ बाएं है तो</target>
      </trans-unit>
